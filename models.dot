	"Comment" -> "ActsAsVotable::Vote" [label="votes_for", arrowtail=odot, arrowhead=crow, dir=both color="#ED9A61"]
	"Category" -> "Catsubcategory" [arrowtail=odot, arrowhead=crow, dir=both color="#BA258F"]
